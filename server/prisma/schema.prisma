
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String 
  username String
  fullName String?
  description String?
  profileImage String?
  isProfileSet Boolean? @default(false)
  createdAt DateTime @default(now())
}
